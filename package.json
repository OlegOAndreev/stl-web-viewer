{
  "name": "stl-web-viewer",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run build:wasm && npm run build:ts",
    "build:wasm": "./wasm/run-build.sh",
    "build:ts": "eslint ./src/** && tsc && vite build",
    "preview": "vite preview",
    "test": "npm run test:ts && npm run test:wasm",
    "test:ts": "vitest run",
    "test:wasm": "cd wasm && cargo test",
    "clippy": "cd wasm && cargo clippy"
  },
  "devDependencies": {
    "@eslint/css": "^0.14.1",
    "@eslint/js": "^9.39.1",
    "@types/three": "^0.181.0",
    "eslint": "^9.39.1",
    "globals": "^16.5.0",
    "jiti": "^2.6.1",
    "lil-gui": "^0.21.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.3",
    "vite": "^7.1.7",
    "vitest": "^4.0.10"
  },
  "dependencies": {
    "binaryen": "^125.0.0",
    "coi-serviceworker": "^0.1.7",
    "three": "^0.181.0"
  }
}
